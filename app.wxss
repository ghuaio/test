/**app.wxss**/
@import 'weui.wxss';
@import 'iconfont.wxss';
page{
    background-color: #F8F8F8;
    font-size: 16px;
    font-family: -apple-system-font,Helvetica Neue,Helvetica,sans-serif;
}
.top{
  margin-top: 140rpx;
  box-sizing: border-box;
  width: 100%;
  padding-left: 15px;
  padding-right: 15px;
  height: 250rpx;
  background: -webkit-linear-gradient(left, #4DACFD, #5080FF); /* Safari 5.1 - 6.0 */
  background: -o-linear-gradient(right, #4DACFD, #5080FF); /* Opera 11.1 - 12.0 */
  background: -moz-linear-gradient(right, #4DACFD, #5080FF); /* Firefox 3.6 - 15 */
  background: linear-gradient(to right, #4DACFD , #5080FF); /* 标准的语法 */ 
}
.heard{
  box-sizing: border-box;
  position: fixed;
  width: 100%;
  top: 0;
  z-index: 2;
  padding-top: 40rpx;
  padding-left: 15px;
  padding-right: 15px;
  line-height: 100rpx;
  color: #fff;
  background: -webkit-linear-gradient(left, #4DACFD, #5080FF); /* Safari 5.1 - 6.0 */
  background: -o-linear-gradient(right, #4DACFD, #5080FF); /* Opera 11.1 - 12.0 */
  background: -moz-linear-gradient(right, #4DACFD, #5080FF); /* Firefox 3.6 - 15 */
  background: linear-gradient(to right, #4DACFD , #5080FF); /* 标准的语法 */ 
}
.flex{
  display:-webkit-box;
  display:-webkit-flex;
  display:flex;
}
.flex_item{
  -webkit-box-flex:1;
  -webkit-flex:1;
  flex:1;
}
.save{
    background-image:url('data:image/png;base64,');
}
.sub{
  background-image:url('data:image/png;base64,');
}